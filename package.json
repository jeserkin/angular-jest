{
  "name": "angular",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.14",
    "@angular/common": "^7.2.5",
    "@angular/compiler": "^7.2.5",
    "@angular/core": "^7.2.5",
    "@angular/forms": "^7.2.5",
    "@angular/platform-browser": "^7.2.5",
    "@angular/platform-browser-dynamic": "^7.2.5",
    "@angular/router": "^7.2.5",
    "core-js": "^2.6.5",
    "jest": "^23.6.0",
    "jest-preset-angular": "^6.0.2",
    "ngx-jest-snapshot-serializer": "^1.2.3",
    "rxjs": "^6.4.0",
    "zone.js": "^0.8.29"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.2",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^23.6.0",
    "jest-preset-angular": "^6.0.2",
    "ngx-jest-snapshot-serializer": "^1.2.3",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png)$": "<rootDir>/test/__mocks__/image.js",
      "^@lib/(.*)$": "<rootDir>/src/lib/$1"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test/setupJest.ts",
    "coverageThreshold": {
      "global": {
        "statements": 99.6,
        "branches": 95.9,
        "functions": 99,
        "lines": 99.8
      }
    },
    "testURL": "http://localhost",
    "collectCoverage": true,
    "cacheDirectory": ".jest/cache",
    "snapshotSerializers": [
      "ngx-jest-snapshot-serializer"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/test/",
      "<rootDir>/src/lib/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "\\.snap$",
      "<rootDir>/node_modules/"
    ]
  }
}